@using RedisSessionStoringExampleApp.Models
@model User

<form asp-controller="Admin" asp-action="Login" method="post">
    @Html.AntiForgeryToken()
    <table>
        <tr>
            <td><p>Введите свой login:</p></td>
            <td><input type="text" name="Login" /> </td>
        </tr>
        <tr>
            <td><p>Введите pwd:</p></td>
            <td><input type="text" name="Password" /> </td>
        </tr>
        <tr>
            <td><p>Введите свой email:</p></td>
            <td><input type="text" name="Email" /> </td>
        </tr>
        <tr>
            <td><p>Введите свою роль:</p></td>
            <td><input type="text" name="Role" /> </td>
        </tr>
        <tr>
            <td><input type="submit" value="Отправить" /> </td>
            <td></td>
        </tr>
    </table>
</form>